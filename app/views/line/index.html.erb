<lineone>

	<script src="https://d3js.org/d3.v4.min.js"></script>
	<script>
		var width = 2300;
		var height = 900;
		var padding = 100

		var canvas = d3.select("lineone")
			.append("svg")
			.attr("width",width)
			.attr("height",height);

		canvas.append("g")
			.append("rect")
				.attr("width",width)
				.attr("height",3)
				.attr("fill","black")
				.attr("x",padding)
				.attr("y",height-10);

		canvas.append("g")
			.append("rect")
				.attr("width",3)
				.attr("height",height)
				.attr("fill","black")
				.attr("x",padding)
				.attr("y",10);				
	</script>

</linetwo>